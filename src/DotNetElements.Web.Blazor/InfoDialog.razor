<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            @DialogInstance.Title
        </MudText>
    </TitleContent>
    <DialogContent>
        @DialogContent
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Default" OnClick="OnClose">Ok</MudButton>
    </DialogActions>
</MudDialog>
